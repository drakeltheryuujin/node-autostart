{
  "name": "node-autostart",
  "version": "1.1.3",
  "description": "Enable autostart for your node apps",
  "main": "lib/index.js",
  "bin": {
    "autostart": "cli.js"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "coverage-ci": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {
    "codecov.io": "0.x.x",
