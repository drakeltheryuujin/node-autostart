    "username": "1.x.x",
    "yargs": "3.x.x"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "mocha": "2.x.x",
    "istanbul": "0.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxrimue/node-autostart.git"
  },
  "keywords": [
    "autostart",
    "startup",
